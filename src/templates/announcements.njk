{% macro announcement_list(announcements) %}
  <h3 class="text-2xl font-bold text-indigo-800 mt-6">
    Announcements:
  </h3>
  <ul class="list-disc list-inside">
    {% for item in announcements.announcements %}
      <li>{{ item.date }} - {{ item.message | safe }}</li>
    {% endfor %}
  </ul>
{% endmacro %}